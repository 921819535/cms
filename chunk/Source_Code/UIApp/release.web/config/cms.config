<?xml version="1.0" encoding="utf-8"?>
<settings>
  <!-- ### 系统配置 ###
       ****************************
       sys_name:                系统名称
       sys_key:                    序列号
       sys_domain:              域名
  -->
  <add key="sys_domain"><![CDATA[http://localhost:8000]]></add>
  <!-- ### 模板设置 ###
       ********************************
       tpl_usecompress:       模板使用压缩
       tpl_usefullpath:          模板使用完整路径
  -->
  <add key="tpl_usecompress"><![CDATA[false]]></add>
  <add key="tpl_usefullpath"><![CDATA[true]]></add>
  <!-- ### 数据库配置 ###
      **************************************************

      db_type:  数据库类型, 可选值: oledb(Access数据库),
                    sqlite,mssql(SQL Server),mysql
                    
      db_conn:  连接字符串,可用$ROOT代替数据库文件根目录
      db_prefix:数据表前缀,默认为"O"
      *************************************************
   -->  <!--
  <add key="db_type">sqlite</add>
  <add key="db_conn">Data Source=$ROOT/data/db.db</add>
 -->
  <add key="db_type">mysql</add>
  <add key="db_conn">server=localhost;database=mydb;uid=root;pwd=$Newmin;charset=utf8</add>

  <!--
  <add key="db_type">mssql</add>
  <add key="db_conn">server=.\sqlexpress;database=opcms;uid=sa;pwd=123000</add>
  -->
  <add key="db_prefix"><![CDATA[qtedu_]]></add>
  <!-- ### 会员配置 ###
       ***********************************
       mm_avatar_path:    会员头像存储路径
       ***********************************
  -->
  <add key="mm_avatar_path">/resources/avatar/</add>
  <add key="sys_autowww"><![CDATA[true]]></add>
  <add key="opti_IndexCacheSeconds"><![CDATA[0]]></add>
  <add key="opti_ClientCacheSeconds"><![CDATA[0]]></add>
  <!--- ### 资料配置 ###
        ***********************
        pro_notice:   网站公告
        pro_slogan:   标语
        pro_tel:      电话
        pro_fax:      传真
        pro_phone:    手机
        pro_address:  地址
        pro_email:    电子邮箱
        pro_qq:       QQ号码
        pro_msn:      MSN账号
        ***********************
  -->
  <add key="pro_notice"><![CDATA[网站公告]]></add>
  <add key="pro_slogan"><![CDATA[网站标语]]></add>
  <add key="pro_tel"><![CDATA[021-66666666]]></add>
  <add key="pro_fax"><![CDATA[021-66666666]]></add>
  <add key="pro_phone"><![CDATA[1861699999]]></add>
  <add key="pro_address"><![CDATA[上海市]]></add>
  <add key="pro_email"><![CDATA[ops@163.com]]></add>
  <add key="pro_qq"><![CDATA[188867734]]></add>
  <add key="pro_msn"><![CDATA[188867734]]></add>
  <add key="license_name"><![CDATA[评估用户]]></add>
  <add key="license_key"><![CDATA[]]></add>
  <add key="server_static_enabled"><![CDATA[false]]></add>
  <add key="sys_admin_tag"><![CDATA[admin]]></add>
  <add key="server_static"><![CDATA[cdn.ops.cc/cms]]></add>
</settings>